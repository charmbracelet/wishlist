Host multiple3
	HostName multi3.foo.local
	User overridden
	SendEnv AAA

Host no.hostname
	Port 23231
	ForwardAgent yes

Host req.tty
	RequestTTY yes

Host remote.cmd
	RemoteCommand tmux

